{
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^test/(.*)$": "<rootDir>/$1"
  },
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/default-timeout.ts"],
  "collectCoverageFrom": ["src/**/*.(t|j)s"],
  "coverageDirectory": "./coverage",
  "testSequencer": "<rootDir>/test-sequencer.js"
}
